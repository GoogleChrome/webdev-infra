{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "lib": ["es2018", "DOM"]
  },
  "include": ["**/*"],
  "exclude": ["tests/**"]
}
